{
  "name": "source-notes",
  "version": "1.2.1",
  "description": "Search source files for TODO, FIXME, OPTIMI(S|Z)E or custom annotation tags",
  "directories": {
    "example": "example",
    "man": "man",
    "test": "test"
  },
  "bin": {
    "source-notes": "bin/cmd.js"
  },
  "scripts": {
    "test": "tape test/*.js"
  },
  "author": "Akiva Levy <akiva@sixthirteen.co> (http://sixthirteen.co/)",
  "repository": {
    "type": "git",
    "url": "git://github.com/akiva/source-notes.git"
  },
  "bugs": {
    "url": "https://github.com/akiva/source-notes/issues"
  },
  "keywords": [
    "notes",
    "source code",
    "annotations",
    "todo",
    "fixme"
  ],
  "license": "MIT",
  "homepage": "https://github.com/akiva/source-notes#readme",
  "dependencies": {
    "chalk": "^0.5.1",
    "minimist": "^1.1.0",
    "pretty-tree": "^0.2.0",
    "readable-stream": "^2.0.4",
    "through2": "^2.0.0"
  },
  "devDependencies": {
    "tape": "^4.2.2"
  }
}
