{
  "camelcase": true,
  "eqeqeq": true,
  "eqnull": true,
  "esnext": true,
  "forin": false,
  "funcscope": true,
  "indent": 2,
  "lastsemic": false,
  "latedef": false,
  "laxbreak": false,
  "laxcomma": false,
  "maxdepth": 3,
  "maxerr": 100,
  "maxlen": 80,
  "maxparams": 3,
  "maxstatements": 15,
  "newcap": false,
  "noarg": true,
  "node": true,
  "noempty": true,
  "passfail": false,
  "quotmark": "single",
  "shadow": false,
  "strict": true,
  "supernew": false,
  "trailing": true,
  "undef": true,
  "unused": true,
  "white": false
}
